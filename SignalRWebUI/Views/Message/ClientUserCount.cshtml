    <section class="food_section layout_padding">
        <div class="container">





            <script type="text/javascript">

                $(document).ready(()=>{
                    var connection= new signalR.HubConnectionBuilder().withUrl("https://localhost:7113/SignalRHub").build();

                    connection.start().then(()=>{
                    }).catch((err)=>{console.log(err) });


                    connection.on("ReceiveClientCount",(value)=>{
                        console.log(value);
                        $("#clientCount").text(value);

                    })

                });
            </script>




            <h4 class="page-title">Anlık Client Sayısı:<strong id="clientCount"></strong> </h4>


        </div>
    </section>

